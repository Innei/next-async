{
  "name": "@innei/next-async",
  "version": "0.4.0",
  "description": "A simple implementation of koa middleware.",
  "author": "Innei",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "readme.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "pnpm": ">=7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache"
    ]
  },
  "bump": {
    "before": [
      "npm run package"
    ],
    "publish": true,
    "changelog": true
  },
  "scripts": {
    "build": "tsdown",
    "package": "NODE_ENV=production npm run build",
    "test": "vitest",
    "prepare": "husky install",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@innei/eslint-config-react-ts": "0.9.4",
    "@innei/eslint-config-ts": "0.9.4",
    "@innei/prettier": "0.9.4",
    "@types/node": "18.7.16",
    "@unocss/preset-wind": "0.45.18",
    "@vitest/coverage-c8": "0.24.3",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "postcss": "8.4.16",
    "postcss-import": "15.0.0",
    "postcss-nested": "5.0.6",
    "prettier": "2.7.1",
    "tsdown": "0.20.0-beta.4",
    "tslib": "2.4.0",
    "typescript": "^5.9.3",
    "unocss": "0.45.18",
    "vite": "3.1.0",
    "vitest": "0.23.1"
  }
}
